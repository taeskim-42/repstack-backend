{
  "name": "repstack-backend",
  "version": "1.0.0",
  "description": "A comprehensive Rails 8 API backend for fitness tracking and AI-powered workout routine generation.",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "schema:dump": "npx get-graphql-schema https://repstack-backend-production.up.railway.app/graphql > schema.graphql",
    "schema:dump:local": "npx get-graphql-schema http://localhost:3000/graphql > schema.graphql",
    "docs:generate": "npx spectaql spectaql.yml",
    "docs:build": "npm run schema:dump && npm run docs:generate",
    "docs:build:local": "npm run schema:dump:local && npm run docs:generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taeskim-42/repstack-backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/taeskim-42/repstack-backend/issues"
  },
  "homepage": "https://github.com/taeskim-42/repstack-backend#readme",
  "devDependencies": {
    "spectaql": "^3.0.6"
  }
}
